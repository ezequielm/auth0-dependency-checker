{
  "env": {
    "node": true
  },
  "rules": {
    "no-bitwise": 0,
    "camelcase": 0,
    "curly": 2,
    "eqeqeq": 2,
    "wrap-iife": 2,
    "indent": [2, 2],
    "no-use-before-define": 2,
    "new-cap": 1,
    "no-caller": 2,
    "quotes": [2, "single"],
    "no-undef": 2,
    "no-unused-vars": 2,
    "strict": 2,
    "no-trailing-spaces": 1,
    "guard-for-in": 2,
    "no-empty": 2,
    "no-new": 2,
    "max-nested-callbacks": [2, 4],
    "complexity": [2, 5],
    "max-len": [2, 120, 4]
  }
}
